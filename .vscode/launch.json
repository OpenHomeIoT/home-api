{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug home-api",
      "port": 9774,
      "skipFiles": [
        "<node_internals>/**"
      ],

      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "debug"],
      "preLaunchTask": "gulp: build:dev",
      "sourceMaps": true
    }
  ]
}
